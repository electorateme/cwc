#!/usr/bin/env ruby

require 'commander/import'
require 'cwc'

program :version, Cwc::VERSION
program :description, 'CLI for interacting with cwc api'
 
command :test do |c|
  c.syntax = 'cwc test'
  c.summary = 'Tests API connection'
  c.description = 'Tests API connection'
  c.action do |args, options|
    puts "This is a test command for CWC gem"
  end
end